  shorttitle = {Cortical Thickness and Sulcal Depth},
  author = {Tosun, Duygu and Siddarth, Prabha and Levitt, Jennifer and Caplan, Rochelle},
  date = {2015-10},
  journaltitle = {BJPsych Open},
