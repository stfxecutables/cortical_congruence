@article{mackinleyDeviantCorticalSulcation2020,
  title = {Deviant Cortical Sulcation Related to Schizophrenia and Cognitive Deficits in the Second Trimester},
  author = {MacKinley, Michael Lloyd and Sabesan, Priyadharshini and Palaniyappan, Lena},
  date = {2020-07-15},
  journaltitle = {Translational Neuroscience},
